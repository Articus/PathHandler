{
  "name": "articus/path-handler",
  "description": "Alternative routing and dispatch middleware for API development with Zend Expressive",
  "keywords": [
    "middleware",
    "API",
    "zend",
    "expressive",
    "routing",
    "dispatch"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Arthur Mogliev"
    }
  ],
  "version": "0.3",
  "require": {
    "php": "^5.6 || ^7.0",
    "doctrine/annotations": "^1.4",
    "zendframework/zend-cache": "^2.7",
    "zendframework/zend-servicemanager": "^3.3",
    "zendframework/zend-expressive": "^2.1",
    "zendframework/zend-http": "^2.7",
    "zendframework/zend-stdlib": "^3.1",
    "http-interop/http-middleware": "^0.4.1"
  },
  "suggest": {
    "zendframework/zend-serializer": "It is required for most Zend Cache adapters to store metadata.",
    "articus/data-transfer": "It is required for Articus\\PathHandler\\Attribute\\Transter and Articus\\PathHandler\\Producer\\Transfer",
    "nikic/fast-route": "It is required for Articus\\PathHandler\\Router\\FastRouteAnnotation",
    "zendframework/zend-router": "It is required for Articus\\PathHandler\\Router\\TreeConfiguration",
    "zendframework/zend-psr7bridge": "It is required for Articus\\PathHandler\\Router\\TreeConfiguration"
  },
  "require-dev": {
    "articus/data-transfer": "*",
    "codeception/base": "*",
    "phpspec/prophecy": "*",
    "nikic/fast-route": "^1.2",
    "zendframework/zend-router": "^3.0",
    "zendframework/zend-psr7bridge": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\PathHandler\\": "tests/functional",
      "Test\\PathHandler\\Sample\\": "tests/_sample"
    }
  },
  "scripts": {
    "test": "codecept run --coverage-xml"
  }
}
